			"event": [
				{
					"listen": "test",
					"script": {
						"exec": [
							"pm.test(\"Status code is 201\", function () {\r",
							"    pm.response.to.have.status(201);\r",
							"});\r",
							"const jsonData = JSON.parse(responseBody);\r",
							"pm.test(\"Check that the product name is Dell Inspiron 505\", function () {\r",
							"    pm.expect(jsonData.name).to.eql(\"Dell Inspiron 505\");\r",
							"});"
